# SmartRouteAI - Environment Variables Example
# Copy this file to .env and fill in your actual values

# API Keys (Get your free API key from: https://openweathermap.org/api)
OPENWEATHER_API_KEY="Your_API_Key"

# Google Maps API Key (Get from: https://console.cloud.google.com/)
GOOGLE_MAPS_API_KEY="Your_API_Key"

# Holiday API Key (Get from: https://www.abstractapi.com/holidays-api)
HOLIDAY_API_KEY="Your_API_Key" 

# Azure Cognitive Services API Key (Get from: https://portal.azure.com/)
AZURE_COGNITIVE_API_KEY="Your_API_Key"

# Service Ports
ML_SERVICE_PORT=5001
BACKEND_PORT=5077
FRONTEND_PORT=4200

# Development Settings
DEBUG=true
ENVIRONMENT=development

# External Services URLs
WEATHER_API_BASE_URL=https://api.openweathermap.org/data/2.5
MAPS_API_BASE_URL=https://maps.googleapis.com/maps/api

# Logging
LOG_LEVEL=INFO
LOG_FILE=smartroute_ai.log 